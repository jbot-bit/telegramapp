run = "python main.py"
modules = ["python-3.12", "postgresql-16"]

[nix]
channel = "stable-24_05"
packages = ["libxcrypt", "postgresql"]

[deployment]
run = ["uvicorn", "main:app", "--host", "0.0.0.0", "--port", "5000"]
deploymentTarget = "autoscale"

[agent]
integrations = ["python_database:1.0.0"]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Server"

[[workflows.workflow]]
name = "Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python main.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 38441
externalPort = 3000

[[ports]]
localPort = 40723
externalPort = 3001
